# Copy this file to application-secret.properties and fill in your values

# Database Password
spring.datasource.password=your_database_password_here

# JWT Configuration
# Generate a secure random base64 string (minimum 64 characters for HS512)
# You can generate one using: openssl rand -base64 64
jwt.secret=YourBase64EncodedSecretKeyHereMinimum64CharactersForHS512AlgorithmSecurityPurpose
jwt.access-token-expiration=3600000
jwt.refresh-token-expiration=604800000

# Kakao OAuth2 Configuration
# Get these from: https://developers.kakao.com/console/app
kakao.client-id=your_kakao_rest_api_key_here
kakao.client-secret=your_kakao_client_secret_here
kakao.redirect-uri=http://localhost:8080/api/auth/kakao/callback
kakao.token-uri=https://kauth.kakao.com/oauth/token
kakao.user-info-uri=https://kapi.kakao.com/v2/user/me
